@model IEnumerable<Channa.Models.Transaksi>

@{
    ViewData["Title"] = "Index";
}

<h1>Home Channa</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayName("IdIkan")
            </th>
            <th>
                @Html.DisplayName("IdPembeli")
            </th>
            <th>
                @Html.DisplayName("JumlahTransaksi")
            </th>
            <th>
                @Html.DisplayName("TanggalTransaksi")
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdIkan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdPembeli)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JumlahTransaksi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TanggalTransaksi)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdTransaksi"><i class="fa fa-marker fa-lg"></i></a> |
                <a asp-action="Details" asp-route-id="@item.IdTransaksi"><i class="fas fa-list"></i></a> |
                <a asp-action="Delete" asp-route-id="@item.IdTransaksi"><i class="fa fa-trash-alt fa-lg"></i></a>
            </td>
        </tr>
}
    </tbody>
</table>
