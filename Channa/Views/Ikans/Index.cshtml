@model IEnumerable<Channa.Models.Ikan>

@{
    ViewData["Title"] = "Index";
}

<h1>Home Channa</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayName("NamaIkan")
            </th>
            <th>
                @Html.DisplayName("StockIkan")
            </th>
            <th>
                @Html.DisplayName("HargaIkan")
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NamaIkan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StockIkan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HargaIkan)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdIkan"><i class="fa fa-marker fa-lg"></i></a> |
                <a asp-action="Details" asp-route-id="@item.IdIkan"><i class="fas fa-list"></i></a> |
                <a asp-action="Delete" asp-route-id="@item.IdIkan"><i class="fa fa-trash-alt fa-lg"></i></a>
            </td>
        </tr>
}
    </tbody>
</table>
